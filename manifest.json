{
  "manifest_version": 3,
  "name": "White Rabbit: New Tab",
  "version": "2.0.1",
  "description": "Discover a Stunning Start to Every Tab",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["storage"],
  "host_permissions": [
    "https://www.google.com/s2/favicons/ *",
    "https://t3.gstatic.com/faviconV2/ *"
  ],
  "action": {
    "default_icon": {
      "48": "icon/48x48.png",
      "128": "icon/128x128.png"
    }
  },
  "icons": {
    "48": "icon/48x48.png",
    "128": "icon/128x128.png"
  }
}